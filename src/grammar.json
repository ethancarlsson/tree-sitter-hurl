{
  "name": "hurl",
  "rules": {
    "entry": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "_request_response"
      }
    },
    "_request_response": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "request"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "response"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "request": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "http_method"
        },
        {
          "type": "SYMBOL",
          "name": "_sp"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "SYMBOL",
          "name": "_lt"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "_header"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "input"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "http_method": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "GET"
        },
        {
          "type": "STRING",
          "value": "HEAD"
        },
        {
          "type": "STRING",
          "value": "POST"
        },
        {
          "type": "STRING",
          "value": "PUT"
        },
        {
          "type": "STRING",
          "value": "DELETE"
        },
        {
          "type": "STRING",
          "value": "CONNECT"
        },
        {
          "type": "STRING",
          "value": "OPTIONS"
        },
        {
          "type": "STRING",
          "value": "TRACE"
        },
        {
          "type": "STRING",
          "value": "PATCH"
        }
      ]
    },
    "_sp": {
      "type": "PATTERN",
      "value": "[ \\t]"
    },
    "_lt": {
      "type": "PATTERN",
      "value": "\\n"
    },
    "url": {
      "type": "PATTERN",
      "value": "(https?:\\/\\/[^\\s]+)"
    },
    "_header": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "SYMBOL",
                "name": "_lt"
              },
              {
                "type": "SYMBOL",
                "name": "key_value"
              },
              {
                "type": "SYMBOL",
                "name": "request_param_keyword"
              }
            ]
          },
          {
            "type": "SYMBOL",
            "name": "_lt"
          }
        ]
      }
    },
    "request_param_keyword": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "[QueryStringParams]"
        },
        {
          "type": "STRING",
          "value": "[FormParams]"
        },
        {
          "type": "STRING",
          "value": "[BasicAuth]"
        },
        {
          "type": "STRING",
          "value": "[Cookies]"
        },
        {
          "type": "STRING",
          "value": "[MultipartFormData]"
        },
        {
          "type": "STRING",
          "value": "[Options]"
        }
      ]
    },
    "variable": {
      "type": "PATTERN",
      "value": "\\{\\{\\S+\\}\\}"
    },
    "key": {
      "type": "PATTERN",
      "value": "(\\w|\\d|-|_)+"
    },
    "value": {
      "type": "CHOICE",
      "members": [
        {
          "type": "PATTERN",
          "value": "(\\w|\\d|-|_|\\\\|\\.|~|\\{|\\}|<|>|`|\\^|%|!|#|\\$|&|'|\"|\\(|\\)|\\*|\\+|,|\\/|:|;|=|\\?|@|\\[|\\]| |\\|)+"
        },
        {
          "type": "SYMBOL",
          "name": "variable"
        }
      ]
    },
    "input": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "json"
        },
        {
          "type": "SYMBOL",
          "name": "oneline_string"
        },
        {
          "type": "SYMBOL",
          "name": "_json_language_hint"
        },
        {
          "type": "SYMBOL",
          "name": "_multiline_string_body"
        },
        {
          "type": "SYMBOL",
          "name": "_xml_language_hint"
        },
        {
          "type": "SYMBOL",
          "name": "_graphql_language_hint"
        },
        {
          "type": "SYMBOL",
          "name": "_base64"
        },
        {
          "type": "SYMBOL",
          "name": "_hex"
        },
        {
          "type": "SYMBOL",
          "name": "_oneline_file"
        }
      ]
    },
    "_multiline_string_body": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```"
        },
        {
          "type": "SYMBOL",
          "name": "multiline_string"
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "_json_language_hint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```json"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "inner_language_hint"
          },
          "named": true,
          "value": "json"
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "_xml_language_hint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```xml"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "inner_language_hint"
          },
          "named": true,
          "value": "xml"
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "_graphql_language_hint": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "```graphql"
        },
        {
          "type": "ALIAS",
          "content": {
            "type": "SYMBOL",
            "name": "inner_language_hint"
          },
          "named": true,
          "value": "graphql"
        },
        {
          "type": "STRING",
          "value": "```"
        }
      ]
    },
    "multiline_string": {
      "type": "PATTERN",
      "value": "[^```]+"
    },
    "oneline_string": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "`"
        },
        {
          "type": "PATTERN",
          "value": "[^`]*"
        },
        {
          "type": "STRING",
          "value": "`"
        }
      ]
    },
    "_base64": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "base64"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "oneline_base64"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "oneline_base64": {
      "type": "PATTERN",
      "value": "[A-Za-z0-9-=+ \\n]+"
    },
    "_hex": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "hex"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "oneline_hex"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "oneline_hex": {
      "type": "PATTERN",
      "value": "[0-9A-Fa-f]+"
    },
    "_oneline_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "file"
        },
        {
          "type": "STRING",
          "value": ","
        },
        {
          "type": "SYMBOL",
          "name": "filename"
        },
        {
          "type": "STRING",
          "value": ";"
        }
      ]
    },
    "filename": {
      "type": "REPEAT1",
      "content": {
        "type": "CHOICE",
        "members": [
          {
            "type": "SYMBOL",
            "name": "filename_escaped_char"
          },
          {
            "type": "SYMBOL",
            "name": "filename_text"
          }
        ]
      }
    },
    "filename_escaped_char": {
      "type": "PATTERN",
      "value": "\\\\(;|#|[ ])"
    },
    "filename_text": {
      "type": "PATTERN",
      "value": "[^#; \\n\\\\]+"
    },
    "json": {
      "type": "PATTERN",
      "value": "\\{(\\s|.)*\\}"
    },
    "inner_language_hint": {
      "type": "PATTERN",
      "value": "[^```]+"
    },
    "response": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version_and_status"
        }
      ]
    },
    "version_and_status": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "http_version"
        },
        {
          "type": "SYMBOL",
          "name": "status"
        }
      ]
    },
    "http_version": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "HTTP/1.0"
        },
        {
          "type": "STRING",
          "value": "HTTP/1.1"
        },
        {
          "type": "STRING",
          "value": "HTTP/2"
        },
        {
          "type": "STRING",
          "value": "HTTP"
        }
      ]
    },
    "status": {
      "type": "PATTERN",
      "value": "[1-5]\\d\\d"
    },
    "key_value": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "key"
        },
        {
          "type": "STRING",
          "value": ":"
        },
        {
          "type": "SYMBOL",
          "name": "value"
        }
      ]
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

