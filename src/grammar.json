{
  "name": "hurl",
  "rules": {
    "source_file": {
      "type": "REPEAT1",
      "content": {
        "type": "SYMBOL",
        "name": "request_response"
      }
    },
    "request_response": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "request"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "response"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "request": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "http_method"
        },
        {
          "type": "SYMBOL",
          "name": "url"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "input"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "http_method": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "GET"
        },
        {
          "type": "STRING",
          "value": "HEAD"
        },
        {
          "type": "STRING",
          "value": "POST"
        },
        {
          "type": "STRING",
          "value": "PUT"
        },
        {
          "type": "STRING",
          "value": "DELETE"
        },
        {
          "type": "STRING",
          "value": "CONNECT"
        },
        {
          "type": "STRING",
          "value": "OPTIONS"
        },
        {
          "type": "STRING",
          "value": "TRACE"
        },
        {
          "type": "STRING",
          "value": "PATCH"
        }
      ]
    },
    "url": {
      "type": "PATTERN",
      "value": "(https?:\\/\\/[^\\s]+)"
    },
    "input": {
      "type": "CHOICE",
      "members": [
        {
          "type": "SYMBOL",
          "name": "json"
        }
      ]
    },
    "json": {
      "type": "PATTERN",
      "value": "\\{(\\s|.)*\\}"
    },
    "response": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version_and_status"
        }
      ]
    },
    "version_and_status": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "http_version"
        },
        {
          "type": "SYMBOL",
          "name": "status"
        }
      ]
    },
    "http_version": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "HTTP/1.0"
        },
        {
          "type": "STRING",
          "value": "HTTP/1.1"
        },
        {
          "type": "STRING",
          "value": "HTTP/2"
        },
        {
          "type": "STRING",
          "value": "HTTP"
        }
      ]
    },
    "status": {
      "type": "PATTERN",
      "value": "[1-5]\\d\\d"
    },
    "comment": {
      "type": "TOKEN",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "STRING",
            "value": "#"
          },
          {
            "type": "PATTERN",
            "value": ".*"
          }
        ]
      }
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    },
    {
      "type": "SYMBOL",
      "name": "comment"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}

